<div id="anniversary-container" class="anniversary-section" style="display: none;">

    </div>

    <script>
    class AnniversaryManager {
        constructor() {
            this.container = document.getElementById('anniversary-container');
            this.updateInterval = 86400000; // 24hr
            this.lastTimestamp = null;
            this.isVisible = false;

            this.init();
        }

        async init() {
            await this.loadAnniversary();
            this.startPeriodicUpdates();

            document.addEventListener('visibilitychange', () => {
                if (document.visibilityState === 'visible') {
                    this.loadAnniversary();
                }
            });
        }

        async loadAnniversary() {
            try {
                const response = await fetch('/api/anniversary/current');
                const data = await response.json();

                if (data.hasAnniversary) {
                    this.renderAnniversary(data.data);
                    this.showContainer();
                    this.lastTimestamp = Math.floor(Date.now() / 1000);
                } else {
                    this.hideContainer();
                }
            } catch (error) {
                console.error('Failed to load anniversary:', error);
            }
        }

        async checkForUpdates() {
            if (!this.lastTimestamp) {
                await this.loadAnniversary();
                return;
            }

            try {
                const response = await fetch(`/api/anniversary/check/${this.lastTimestamp}`);
                const data = await response.json();

                if (data.hasUpdates) {
                    if (data.data) {
                        this.renderAnniversary(data.data);
                        this.showContainer();
                    } else {
                        this.hideContainer();
                    }
                    this.lastTimestamp = data.timestamp;
                }
            } catch (error) {
                console.error('Failed to check for updates:', error);
                await this.loadAnniversary();
            }
        }

        renderAnniversary(data) {
            let banner = "banner";
            if (data.Type == "Death Anniversary") {
                banner = "";
            }
            console.log(banner, data.Type);
            const html = `
            <div class="pt-120 pb-20 col-12 lqd-section bg-cover bg-center ${banner} bg-style-birthday">
                <div class="container">
                    <div class="row">
                        <div class="col col-12 col-md-6">
                            <div class="h-vh-50 px-0 sticky top-100 left-0 overflow-hidden mb-30">
                                <div class="text-center">
                                    <figure>
                                        ${this.renderImage(data)}
                                    </figure>
                                </div>
                            </div>
                        </div>
                        <div class="col col-12 col-md-6 col-xl-5 offset-xl-1">
                            <div class="relative flex flex-col justify-center items-center text-center h-full pl-15">
                                <h2 class="ld-fh-element mb-0/4em text-65 leading-0/85em aiub-blue-text">
                                    ${data.Title}
                                </h2>
                                <p class="ld-fh-element mb-2em text-16 leading-1/5em text-black">
                                    ${data.Content || ''}
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        `;

            this.container.innerHTML = html;
        }

        renderImage(data) {
            if (data.HasCustomImage && data.ImagePath) {
                return `<img alt="${data.Title}" class="img-responsive img-thumbnail single-image" src="${data.ImagePath}" />`;
            } else if (data.ImageUrl && data.PostUrl) {
                return `
                <a href="/${data.PostUrl}">
                    <img width="400" class="img-responsive img-thumbnail single-image"
                         src="${data.ImageUrl.replace('~', '')}" alt="${data.PostTitle || data.Title}">
                </a>
            `;
            }
            return '';
        }

        showContainer() {
            if (!this.isVisible) {
                this.container.style.display = 'block';
                this.container.classList.add('fade-in');
                this.isVisible = true;
            }
        }

        hideContainer() {
            if (this.isVisible) {
                this.container.classList.add('fade-out');
                setTimeout(() => {
                    this.container.style.display = 'none';
                    this.container.classList.remove('fade-in', 'fade-out');
                }, 300);
                this.isVisible = false;
            }
        }

        startPeriodicUpdates() {
            setInterval(() => {
                if (!document.hidden) { // Only update when page is visible
                    this.checkForUpdates();
                }
            }, this.updateInterval);
        }
    }


