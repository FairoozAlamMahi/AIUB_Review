<div class="container" id="countdown-container"></div>
<script>

    document.addEventListener('DOMContentLoaded', function () {
        fetch('/api/countdown/all')
            .then(response => response.json())
            .then(data => {
                data.forEach(item => renderCountdown(item));
            });
    });

    function renderCountdown(item) {
        const container = document.createElement('div');
        container.classList.add('countdown', item.Type);
        container.dataset.date = item.TargetDate;

        // Add click handler for countdown detail URL
        if (item.CountdownDetailUrl) {
            container.addEventListener('click', function () {
                window.location.href = item.CountdownDetailUrl;
            });
        }

        if (item.Type === 'image-left') {
            const imageWrapper = document.createElement('div');
            imageWrapper.classList.add('image-wrapper');

            const img = document.createElement('img');
            img.src = item.ImageUrl;
            imageWrapper.appendChild(img);

            container.appendChild(imageWrapper);

            const content = document.createElement('div');
            content.classList.add('content');

            const title = document.createElement('h2');
            title.textContent = item.Title;
            title.classList.add('text-left');
            content.appendChild(title);

            const description = document.createElement('div');
            description.textContent = item.Description;
            description.classList.add('text-13');
            description.classList.add('pb-10');
            content.appendChild(description);

            const timer = document.createElement('div');
            timer.classList.add('timer');
            timer.classList.add('text-center');
            timer.id = `timer-${item.Id}`;
            timer.textContent = 'Loading...';
            content.appendChild(timer);

            const location = document.createElement('div');
            location.textContent = item.Location;
            location.classList.add('text-11');
            location.classList.add('pb-10');
            //location.classList.add('text-white');
            location.classList.add('z-2');
            content.appendChild(location)

            container.appendChild(content);
        } else if (item.Type === 'background') {
            container.style.backgroundImage = `url('${item.ImageUrl}')`;
            const title = document.createElement('h2');
            title.textContent = item.Title;
            container.appendChild(title);

            const description = document.createElement('div');
            description.textContent = item.Description;
            description.classList.add('text-13');
            description.classList.add('pb-10');
            description.classList.add('text-white');
            description.classList.add('z-2');
            container.appendChild(description);

            const timer = document.createElement('div');
            timer.classList.add('timer');
            timer.id = `timer-${item.Id}`;
            timer.textContent = 'Loading...';
            container.appendChild(timer);

            const location = document.createElement('div');
            location.textContent = item.Location;
            location.classList.add('text-11');
            location.classList.add('pb-10');
            location.classList.add('text-white');
            location.classList.add('z-2');
            container.appendChild(location)
        }

        document.getElementById('countdown-container').appendChild(container);
        startTimer(item.Id, new Date(item.TargetDate));
    }

    function startTimer(id, targetDate) {
        function update() {
            const now = new Date();
            const diff = targetDate - now;
            if (diff < 0) {
                document.getElementById(`timer-${id}`).textContent = "Started...";
                return;
            }

            const d = Math.floor(diff / (1000 * 60 * 60 * 24)).toString().padStart(2, '0');
            const h = Math.floor((diff / (1000 * 60 * 60)) % 24).toString().padStart(2, '0');
            const m = Math.floor((diff / (1000 * 60)) % 60).toString().padStart(2, '0');
            const s = Math.floor((diff / 1000) % 60).toString().padStart(2, '0');

            document.getElementById(`timer-${id}`).innerHTML = `<div class="text-50 text-left flex">
    <div class='text-center'>
        <div class="font-bold">${d}</div>
        <div class='text-11'>DAYS</div>
    </div>
   <div class='time-colon px-10'>:</div>
    <div class='text-center'>
        <div class="font-bold">${h}</div>
        <div class='text-11'>HOURS</div>
    </div>
    <div class='time-colon px-10'>:</div>
    <div class='text-center'>
        <div class="font-bold">${m}</div>
        <div class='text-11'>MINUTES</div>
    </div>
    <div class='time-colon px-10'>:</div>
    <div class='text-center'>
        <div class="font-bold">${s}</div>
        <div class='text-11'>SECONDS</div>
    </div>
</div>`;
        }

        update();
        setInterval(update, 1000);
    }

    function createMediaItem(src) {
        const mediaItem = document.createElement('div');
        mediaItem.classList.add('ld-media-item', 'pos-rel', 'overflow-hidden', 'h-full');

        // Create the figure element
        const figure = document.createElement('figure');
        figure.classList.add('bg-cover', 'h-300');

        // Create the img element
        const img = document.createElement('img');
        img.classList.add('objfit-cover', 'objpos-center');
        img.src = src; // Use the src variable
        img.alt = "image box";

        // Append elements
        figure.appendChild(img);
        mediaItem.appendChild(figure);

        return mediaItem;
    }

</script>

