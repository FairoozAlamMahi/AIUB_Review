<script>
const scrollDiv = document.querySelector('div.h-690:nth-child(1)');
const sliderDiv = document.getElementById("banner");
const sliderHeight = sliderDiv.offsetHeight;
function changeHeight(change) {
 const computedStyle = window.getComputedStyle(scrollDiv);
 let currentHeight = parseInt(computedStyle.height);
 
 let newHeight = currentHeight + change;

 // Set minimum and maximum height limits
 const minHeight =500;
 const maxHeight = 690;
						
 if (newHeight < minHeight ) {
 newHeight = minHeight;
 } else if (newHeight > maxHeight) {
 newHeight = maxHeight;
 }

 scrollDiv.style.height = newHeight + 'px';
 }
let lastScrollTop = 0;

				
 window.onscroll = function() {
 let scrollPosition = window.scrollY;
 //console.log("sliderHeight ",sliderHeight);
 //console.log("scrollPosition ",scrollPosition);
 if (scrollPosition > lastScrollTop && scrollPosition>sliderHeight) {
								
 // Scrolling Down
 changeHeight(-1); // Increase height by 100px
 } else if(scrollPosition < lastScrollTop && scrollPosition>sliderHeight){
 // Scrolling Up
 changeHeight(1); // Decrease height by 100px
								
 }

 lastScrollTop = scrollPosition;
 };
</script>

    
